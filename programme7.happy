(fun (main) (
	(write (matSum 12))
	(write (matSum 23))
))

(fun (matSum n) (
	(set sum 0 )
	(set line 1)
	(while (<= line n ) (
		(set i 1)
		(while (<= i n) (
			( set sum (+ sum (* line i)))
			( set i (+ i 1))
		))
		(set line (+ line 1))
	))
	(return sum)
))
