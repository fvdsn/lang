(fun (main) (
	(set i 0)
	(while (<=i 10) (
		(write (pow 2 i))
		(- i 1)
	))
))

(fun (pow x n) (
	(set res 1)
	(set i 1)
	(while (<= i n) (
		(set res (* res x))
		(+ i 1)
	))
	(return res)	
))
		
