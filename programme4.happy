(fun (isPrime n) (
	(set isPrime true)
	(set i 2)
	(while (and (< i n) (= isPrime true)) (
		(if (= (% n i) 0) (
			(set isPrime false)
		))
		(set i (+ i 1))
	))
	(return isPrime)
))


